<!doctype html>
<html lang="en">
  <head>
    <script async src="https://www.googletagmanager.com/gtag/js?id=UA-74515875-1"></script>
    <script>
      window.dataLayer = window.dataLayer || [];
      function gtag(){dataLayer.push(arguments);}
      gtag('js', new Date());
      gtag('config', 'UA-74515875-1');
    </script>
    <meta charset="utf-8">
    <meta name="viewport" content="width=device-width, initial-scale=1.0">
    <meta name="description" content="{{ renderData.description or description or metadata.description }}">
    {% set css %}{% include "css/index.css" %}{% endset %}
    <style>{{ css | cssmin | safe }}</style>
    <link rel="stylesheet" href="/css/prism-atom-dark.css" />
    <title>{{ title }}</title>
  </head>
  <body>
    {% include "layouts/header.njk" %}
    <main>
      <div class="content">
        {{ content | safe }}
      </div>
    </main>
    {% include "layouts/footer.njk" %}
  </body>
</html>
